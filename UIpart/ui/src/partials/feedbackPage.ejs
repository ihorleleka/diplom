          <div class="col-md-4 col-md-offset-4 col-xs-12">          
            <label>Ім'я</label>
            <input type="text" id="name" placeholder="Ім'я та фамілія">
            <label>Емейл</label>
            <input type="email" id="email" placeholder="Ваш email">
            <label>Комментар</label>
            <textarea id="comment" cols="32" rows="5" placeholder="Будь ласка залиште свій коментар"></textarea>
          </div>
